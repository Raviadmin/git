<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:BW="java://com.tibco.pe.core.JavaCustomXPATHFunctions" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://schemas.wynnlasvegas.com/Common/Error-Handling/1.0/" xmlns:ns12="http://www.tibco.com/namespaces/tnt/plugins/jms" xmlns:ns5="http://www.wvi.org/schemas/Common/CommonLE/RequestHeader/V1" xmlns:ns6="http://www.tibco.com/pe/EngineTypes" xmlns:pfx="com/tibco/pe/commands" xmlns:ns7="http://www.wvi.org/schemas/Common/CommonLE/LogRequest/V1" xmlns:ns2="http://www.tibco.com/CommonLE2/namespace/Public/Logging/LogRequest.xsd" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns1="http://www.tibco.com/pe/DeployedVarsType" xmlns:ns4="http://www.tibco.com/CommonLE2/namespace/Public/Exception/ExceptionRequest.xsd" xmlns:ns3="http://www.tibco.com/CommonLE2/namespace/Public/Common/RequestHeader.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pfx4="http://www.wvi.org/schemas/Common/CommonLE/ExceptionRequest/V1" xmlns:pfx3="http://www.wvi.org/schemas/Common/CommonLE/ExceptionServerRequest/V1" xmlns:pfx2="http://schemas.wynnlasvegas.com/Common/Logging/1.0/">
    <xsd:import namespace="http://www.wvi.org/schemas/Common/CommonLE/ExceptionRequest/V1" schemaLocation="/SharedResources/SchemaDefinitions/Technical/Common/ExceptionHandling/ExceptionRequest.xsd"/>
    <xsd:import namespace="http://www.wvi.org/schemas/Common/CommonLE/ExceptionRequest/V1" schemaLocation="/SharedResources/SchemaDefinitions/Public/Exception/ExceptionRequest.xsd"/>
    <xsd:import namespace="http://www.wvi.org/schemas/Common/CommonLE/ExceptionServerRequest/V1" schemaLocation="/SharedResources/SchemaDefinitions/Public/Exception/ExceptionServerRequest.xsd"/>
    <pd:name>BusinessProcesses/SubProcesses/ExceptionClient.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx3:ExceptionRequest"/>
    <pd:startX>71</pd:startX>
    <pd:startY>88</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>508</pd:endX>
    <pd:endY>88</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1367344653005</pd:targetNamespace>
    <pd:activity name="Render Exception Request">
        <pd:type>com.tibco.plugin.xml.XMLRendererActivity</pd:type>
        <pd:resourceType>ae.activities.XMLRendererActivity</pd:resourceType>
        <pd:x>200</pd:x>
        <pd:y>96</pd:y>
        <config>
            <renderAsText>text</renderAsText>
            <validateInput>true</validateInput>
            <textEncoding>UTF-8</textEncoding>
            <term ref="pfx3:ExceptionRequest"/>
        </config>
        <pd:inputBindings>
            <xsl:copy-of select="$Start/pfx3:ExceptionRequest"/>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Send Exception to CLE">
        <pd:type>com.tibco.plugin.jms.JMSQueueSendActivity</pd:type>
        <pd:resourceType>ae.activities.JMSQueueSendActivity</pd:resourceType>
        <pd:x>374</pd:x>
        <pd:y>95</pd:y>
        <config>
            <PermittedMessageType>Text</PermittedMessageType>
            <SessionAttributes>
                <transacted>false</transacted>
                <acknowledgeMode>1</acknowledgeMode>
                <maxSessions>1</maxSessions>
                <destination>%%HorizonCM_SiStDataSync/JmsSvrCfg/Destinations/CLEExceptionQueue%%</destination>
            </SessionAttributes>
            <ConfigurableHeaders>
                <JMSDeliveryMode>PERSISTENT</JMSDeliveryMode>
                <JMSExpiration>0</JMSExpiration>
                <JMSPriority>4</JMSPriority>
            </ConfigurableHeaders>
            <ApplicationProperties/>
            <ConnectionReference>/SharedResources/Connections/JMS/SingleSTEP_JMS.sharedjmscon</ConnectionReference>
        </config>
        <pd:inputBindings>
            <ns12:ActivityInput>
                <Body>
                    <xsl:value-of select="$Render-Exception-Request/xmlString"/>
                </Body>
            </ns12:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
